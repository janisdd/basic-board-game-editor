    "@types/react-redux": "4.4.40",
    "@types/redux-actions": "1.2.7",
    "@types/redux-logger": "3.0.0",
    "@types/redux-promise-middleware": "0.0.8",
    "@types/seedrandom": "2.4.27",
    "@types/spark-md5": "3.0.0",
    "ace-builds": "1.3.3",
    "awesome-typescript-loader": "3.1.3",
    "bezier-js": "2.2.5",
